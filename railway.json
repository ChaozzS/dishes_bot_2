{
  "services": [
    {
      "name": "frontend",
      "path": "frontend",
      "build": {
        "builder": "Node.js",
        "buildCommand": "npm install",
        "startCommand": "npm start"
      }
    },
    {
      "name": "backend",
      "path": "backend",
      "build": {
        "builder": "Python 3.9",
        "buildCommand": "pip install -r requirements.txt",
        "startCommand": "python bot.py"
      }
    }
  ]
}
